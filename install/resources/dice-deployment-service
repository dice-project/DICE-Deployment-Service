server {
  listen 80 default_server;
  server_name _;

  location = /favicon.ico {
    access_log off;
    log_not_found off;
  }
  location /static/ {
    root /home/ubuntu/dice_deploy_django;
  }

  location / {
    include uwsgi_params;
    uwsgi_pass unix:/home/ubuntu/dice_deploy_django/nginx.socket;
  }
}
